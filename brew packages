--install/bin/bash -c "$(curl -fsSL https://raw.githubusercontent.com/Homebrew/install/master/install.sh)"

brew tap AdoptOpenJDK/openjdk
mkdir ~/.jenv/versions

# brew
brew install wget
brew install jenv
brew install zsh-autosuggestions

# brew casks
brew cask install sublime
brew cask install java
brew cask install adoptopenjdk
brew cask install adoptopenjdk<version>

jenv add /Library/Java/JavaVirtualMachines/adoptopenjdk-*.jdk/Contents/Home
